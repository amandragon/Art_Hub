<%= render :partial => 'layouts/header' %>

<div class="index">

<h3>Reviews of this Post</h3>

<% @reviews.each do |review| %>
  <div class="row">
     <div class="span3 offset1">
     	<ul>
     	<li><a href="<%=review_path(review)%>"><%=review.title%></a></li>

     </ul>

</div>
  </div>
<% end %>
</div>